<div class="main actor-index">
<div class="container"> 
	<div class="row">
	<% @actors.each do |a| %>

		<div class="actor col-xs-2">

		<%= image_tag a.image %>

		<h3><%= a.first_name %>&nbsp;<%= a.last_name %></h3>
		<p><%= link_to "Learn more", actor_path(a) %></p>

		</div>
	<% end %>
</div>
</div>
</div>
